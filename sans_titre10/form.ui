   <property name="styleSheet">
    <string notr="true">/* Sidebar Container Styling */
#sidebarWidget {
    background-color: #FFFFFF;
    min-width: 200px;
    max-width: 250px;
    border-right: 1px solid #E0E0E0; /* Ligne séparatrice subtile */
    box-shadow: 2px 0px 5px rgba(0, 0, 0, 0.1); /* Légère ombre pour un effet 3D */
}

/* Sidebar List Widget */
#sidebarListWidget {
    background-color: #F5F5F5;;
    border: 2px solid #F5F7FA; /* Bordure avec la couleur demandée */
    border-radius: 8px; /* Coins arrondis pour une touche élégante */
    padding: 5px; /* Espacement interne pour les éléments */
}

/* List Items */
#sidebarListWidget::item {
    padding: 12px 20px;
    color: #555555;
    min-height: 30px;
    border-radius: 6px; /* Coins arrondis pour chaque élément */
    transition: background-color 0.2s ease, padding-left 0.2s ease; /* Animation fluide */
}

/* Selected Item */
#sidebarListWidget::item:selected {
    background-color: #E3F2FD; /* Bleu clair */
    color: #0D47A1; /* Bleu foncé pour le texte */
    border-left: 4px solid #0D47A1; /* Bordure accentuée à gauche */
    font-weight: bold;
    padding-left: 22px; /* Légère animation pour l'effet de sélection */
    box-shadow: inset 0 0 5px rgba(13, 71, 161, 0.2); /* Ombre subtile à l'intérieur */
}

/* Hover (non-sélectionné) */
#sidebarListWidget::item:hover:!selected {
    background-color: #F9F9F9; /* Arrière-plan doux */
    border-left: 3px solid #CCCCCC; /* Accent visuel discret */
}
</string>
